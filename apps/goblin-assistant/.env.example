# Backend Configuration
VITE_BACKEND_URL=http://localhost:8000
VITE_API_BASE_URL=http://localhost:8000/api
VITE_FASTAPI_URL=http://localhost:8001

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_MOCK_API=false
VITE_ENABLE_ANALYTICS=false
VITE_DEBUG_MODE=false
VITE_FEATURE_RAG_ENABLED=true
VITE_FEATURE_MULTI_PROVIDER=true
VITE_FEATURE_PASSKEY_AUTH=true
VITE_FEATURE_GOOGLE_AUTH=false
VITE_FEATURE_ORCHESTRATION=true
VITE_FEATURE_SANDBOX=false

# Turnstile Bot Protection (Cloudflare)
# Get keys from: https://dash.cloudflare.com/turnstile
# IMPORTANT: These are PUBLIC keys only - private keys must be on backend
VITE_TURNSTILE_SITE_KEY=your_turnstile_site_key_here
VITE_TURNSTILE_SITE_KEY_CHAT=
VITE_TURNSTILE_SITE_KEY_LOGIN=
VITE_TURNSTILE_SITE_KEY_SEARCH=

# Optional: Analytics
VITE_ANALYTICS_ID=

# Development
VITE_DEBUG_MODE=false
